{
  "expo": {
    "name": "ListaRotativa",
    "slug": "ListaRotativa",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "ListaRotativa",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#091450"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "dependencies": {
      "react-native-web": "~0.19.6"
    }
  }
}
